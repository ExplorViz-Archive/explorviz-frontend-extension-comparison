<div id='versionchart' class='versionbar' vis='hide'/>

  <div class="container-fluid" style="position: fixed; width: 100%; bottom: 80px; background:white;">
    <div class="row">
      <div class="col-sm-offset-5 col-sm-2 text-center">
        <a {{action 'toggleVersionbarSelect'}}>
          <span id="toggleVersionbarButton" class="glyphicon glyphicon-collapse-up"></span>
        </a>
        <a {{action 'selectTimestamp'}}>
          <span id="selectTimestampButton">Compare</span>
        </a>
      </div>
    </div>
  </div>
